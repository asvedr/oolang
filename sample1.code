class single Main {
	pub a : real;
	pub fn curry_add(a : int) : Fn(real):int {
		fn closure(b : real) : int {
			return a + b as int;
		};
		return closure
	};
	priv fn add(p : int) {
		self.a = self.a + p;
	};
}

fn main() : Fn(int):int {
	var b = 2;
	fn fact_plus(a : int) : int {
		if a < 1 {
			return b;
		} else {
			return fact_plus(a - 1) * a;
		}
	};
	return fact_plus;
}
